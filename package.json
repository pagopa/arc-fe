{
  "name": "pagopa-arc-fe",
  "version": "1.0.0",
  "repository": "https://github.com/pagopa/pagopa-arc-fe.git",
  "author": "pagoPA",
  "license": "MIT",
  "source": "src/index.html",
  "scripts": {
    "generate-api-client": "swagger-typescript-api -p https://raw.githubusercontent.com/pagopa/pagopa-arc-infra-api-spec/main/src/cittadini/api/cittadini/v1/area_riservata_cittadino_openapi.yml -o ./generated -n apiClient --axios",
    "generate-data-contracts": "swagger-typescript-api  --modular --no-client -p https://raw.githubusercontent.com/pagopa/pagopa-arc-infra-api-spec/main/src/cittadini/api/cittadini/v1/area_riservata_cittadino_openapi.yml -o ./generated",
    "generate-zod-schema": "yarn ts-to-zod",
    "generate": "yarn generate-api-client && yarn generate-data-contracts && yarn generate-zod-schema",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx",
    "lint:fix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "start": "yarn generate && parcel --no-cache",
    "build": "yarn generate && tsc & parcel build",
    "type-check": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "coverage": "jest --coverage",
    "axe": "axe localhost:1234, localhost:1234/transactions, localhost:1234/transaction/1, localhost:1234/user,  --include '#root'",
    "serve-coverage-html-report": "npx serve coverage/lcov-report -c ./../../serve.json"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.9.1",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@chromatic-com/storybook": "^1.3.1",
    "@storybook/addon-essentials": "^8.0.6",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.0.6",
    "@storybook/addon-onboarding": "^8.0.6",
    "@storybook/blocks": "^8.0.6",
    "@storybook/react": "^8.0.6",
    "@storybook/react-vite": "^8.0.6",
    "@storybook/test": "^8.0.6",
    "@testing-library/react": "^14.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "autochangelog": "^3.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "latest",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "parcel-resolver-ignore": "^2.2.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "storybook": "^8.0.6",
    "swagger-typescript-api": "^13.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-to-zod": "^3.8.5",
    "tsconfig.json": "^1.0.11",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@pagopa/mui-italia": "^1.3.0",
    "@tanstack/react-query": "^5.40.0",
    "axios": "^1.6.8",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3"
  },
  "packageManager": "yarn@4.1.1",
  "parcelIgnore": [
    "otSDKStub.js"
  ]
}
